{
  "expo": {
    "name": "portal-noticias",
    "slug": "portal-noticias",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/news.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/news.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/news.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.noticias.app",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}